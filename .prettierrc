{
  "arrowParens": "always",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "overrides": [
    {
      "files": ["*.json", "*.yml", "*.md"],
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
